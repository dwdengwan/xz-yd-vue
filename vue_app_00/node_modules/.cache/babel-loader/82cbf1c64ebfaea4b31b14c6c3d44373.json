no=\"+pno+\"&pageSize=\"+ps;\r\n                this.axios.get(url).then(result=>{\r\n                    console.log(result.data);\r\n                    this.list=result.data.data;\r\n                    this.pageCount=result.data.pageCount;\r\n                })\r\n                // 3:返回数据保存 list\r\n                // 4:保存总页数\r\n                // \r\n                // 5:在模板中创建循环显示list中数据\r\n            },\r\n            change(){\r\n                this.point++;\r\n            }\r\n        },\r\n        created(){\r\n            this.getMore();\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n.app-news .mui-table-view-cell .mui-ellipsis{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    font-size:12px;\r\n    color:#226aff;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\home"}]}